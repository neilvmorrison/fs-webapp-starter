{
  "name": "cofoundry-ai",
  "version": "0.0.1",
  "description": "A simple note taker app to get you off the subscriptions",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server:add": "pnpm add --filter server",
    "server:dev": "pnpm --dir ./packages/server run dev",
    "shared:build": "pnpm --dir ./packages/shared run build",
    "shared:add": "pnpm --dir ./packages/shared add",
    "web:add": "pnpm add --filter web",
    "web:dev": "pnpm --dir ./packages/web run dev",
    "web:build": "pnpm --dir ./packages/web run build",
    "dev": "concurrently \"pnpm --dir ./packages/web run dev\" \"pnpm --dir ./packages/server run dev\""
  },
  "keywords": [],
  "author": "Neil Morrison",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "typescript": "^5.8.3"
  }
}
